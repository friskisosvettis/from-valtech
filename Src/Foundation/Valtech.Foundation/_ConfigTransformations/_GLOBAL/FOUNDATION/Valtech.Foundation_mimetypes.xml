          <remove fileExtension=".svg"/>
        </Item>
        <Item path="configuration/system.webServer/staticContent//mimeMap[@fileExtension='.svg']" action="InsertIfMissing">
          <mimeMap fileExtension=".svg" mimeType="image/svg+xml"/>
        </Item>
        <!-- svgz -->
        <Item path="configuration/system.webServer/staticContent//remove[@fileExtension='.svgz']" action="InsertIfMissing">
          <remove fileExtension=".svgz"/>
        </Item>
        <Item path="configuration/system.webServer/staticContent//mimeMap[@fileExtension='.svgz']" action="InsertIfMissing">
          <mimeMap fileExtension=".svgz" mimeType="image/svg+xml"/>
        </Item>
        <!-- eot -->
        <Item path="configuration/system.webServer/staticContent//remove[@fileExtension='.eot']" action="InsertIfMissing">
          <remove fileExtension=".eot"/>
        </Item>
        <Item path="configuration/system.webServer/staticContent//mimeMap[@fileExtension='.eot']" action="InsertIfMissing">
          <mimeMap fileExtension=".eot" mimeType="application/vnd.ms-fontobject"/>
