<?xml version="1.0"?>
<Deployment>
  <Website>
    <ReplaceXml>
      <File name="web.config">
        <!-- Static Content setup - mimetypes -->
        <!-- mp4 -->
        <Item path="configuration/system.webServer/staticContent//remove[@fileExtension='.mp4']" action="InsertIfMissing">
          <remove fileExtension=".mp4"/>
        </Item>
        <Item path="configuration/system.webServer/staticContent//mimeMap[@fileExtension='.mp4']" action="InsertIfMissing">
          <mimeMap fileExtension=".mp4" mimeType="video/mp4"/>
        </Item>
        <!-- m4v -->
        <Item path="configuration/system.webServer/staticContent//remove[@fileExtension='.m4v']" action="InsertIfMissing">
          <remove fileExtension=".m4v"/>
        </Item>
        <Item path="configuration/system.webServer/staticContent//mimeMap[@fileExtension='.m4v']" action="InsertIfMissing">
          <mimeMap fileExtension=".m4v" mimeType="video/m4v"/>
        </Item>
        <!-- ogg -->
        <Item path="configuration/system.webServer/staticContent//remove[@fileExtension='.ogg']" action="InsertIfMissing">
          <remove fileExtension=".ogg"/>
        </Item>
        <Item path="configuration/system.webServer/staticContent//mimeMap[@fileExtension='.ogg']" action="InsertIfMissing">
          <mimeMap fileExtension=".ogg" mimeType="video/ogg"/>
        </Item>
        <!-- ogv -->
        <Item path="configuration/system.webServer/staticContent//remove[@fileExtension='.ogv']" action="InsertIfMissing">
          <remove fileExtension=".ogv"/>
        </Item>
        <Item path="configuration/system.webServer/staticContent//mimeMap[@fileExtension='.ogv']" action="InsertIfMissing">
          <mimeMap fileExtension=".ogv" mimeType="video/ogg"/>
        </Item>
        <!-- webm -->
        <Item path="configuration/system.webServer/staticContent//remove[@fileExtension='.webm']" action="InsertIfMissing">
          <remove fileExtension=".webm"/>
        </Item>
        <Item path="configuration/system.webServer/staticContent//mimeMap[@fileExtension='.webm']" action="InsertIfMissing">
          <mimeMap fileExtension=".webm" mimeType="video/webm"/>
        </Item>
        <!-- oga -->
        <Item path="configuration/system.webServer/staticContent//remove[@fileExtension='.oga']" action="InsertIfMissing">
          <remove fileExtension=".oga"/>
        </Item>
        <Item path="configuration/system.webServer/staticContent//mimeMap[@fileExtension='.oga']" action="InsertIfMissing">
          <mimeMap fileExtension=".oga" mimeType="audio/ogg"/>
        </Item>
        <!-- spx -->
        <Item path="configuration/system.webServer/staticContent//remove[@fileExtension='.spx']" action="InsertIfMissing">
          <remove fileExtension=".spx"/>
