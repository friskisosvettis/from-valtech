          <mimeMap fileExtension=".otf" mimeType="font/otf"/>
        </Item>
        <!-- woff -->
        <Item path="configuration/system.webServer/staticContent//remove[@fileExtension='.woff']" action="InsertIfMissing">
          <remove fileExtension=".woff"/>
        </Item>
        <Item path="configuration/system.webServer/staticContent//mimeMap[@fileExtension='.woff']" action="InsertIfMissing">
          <mimeMap fileExtension=".woff" mimeType="font/x-woff"/>
        </Item>
        <!-- woff2 -->
        <Item path="configuration/system.webServer/staticContent//remove[@fileExtension='.woff2']" action="InsertIfMissing">
          <remove fileExtension=".woff2"/>
        </Item>
        <Item path="configuration/system.webServer/staticContent//mimeMap[@fileExtension='.woff2']" action="InsertIfMissing">
          <mimeMap fileExtension=".woff2" mimeType="application/font-woff2"/>
        </Item>
      </File>
    </ReplaceXml>
  </Website>
</Deployment>